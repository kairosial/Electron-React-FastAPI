# FaceFusion Configuration
# GPU 사용 여부 (gpu or cpu)
FACEFUSION_MODE=cpu

# 실행 프로바이더
# GPU가 있다면: CUDAExecutionProvider,CPUExecutionProvider
# CPU만 있다면: CPUExecutionProvider
FACEFUSION_EXECUTION_PROVIDERS=CPUExecutionProvider

# FaceFusion 모델 경로
FACEFUSION_MODEL_PATH=../models

# 타겟 이미지 디렉토리
PROFILE_TARGETS_DIR=../assets/profile_targets
TALENT_TARGETS_DIR=../assets/talent_targets

# 처리 제한 시간 (초)
MAX_PROCESSING_TIME=30

# 얼굴 향상 모델 (gfpgan, codeformer, none)
FACE_ENHANCER=gfpgan

# 얼굴 감지 모델 (retinaface, yunet)
FACE_DETECTOR=retinaface

# FaceFusion 라이브러리 경로
FACEFUSION_PATH=/Users/syk/PC/git/facefusion
